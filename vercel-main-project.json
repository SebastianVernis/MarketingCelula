{
  "version": 2,
  "name": "grupomusicalcelula-main",
  "rewrites": [
    {
      "source": "/bodas",
      "destination": "https://marketing-celula.vercel.app/bodas"
    },
    {
      "source": "/xv",
      "destination": "https://marketing-celula.vercel.app/xv"
    },
    {
      "source": "/privada",
      "destination": "https://marketing-celula.vercel.app/privada"
    },
    {
      "source": "/assets/gallery/:path*",
      "destination": "https://marketing-celula.vercel.app/assets/gallery/:path*"
    },
    {
      "source": "/assets/icons/:path*",
      "destination": "https://marketing-celula.vercel.app/assets/icons/:path*"
    },
    {
      "source": "/assets/logo/:path*",
      "destination": "https://marketing-celula.vercel.app/assets/logo/:path*"
    },
    {
      "source": "/assets/fonts/:path*",
      "destination": "https://marketing-celula.vercel.app/assets/fonts/:path*"
    },
    {
      "source": "/css/:path*",
      "destination": "https://marketing-celula.vercel.app/css/:path*"
    },
    {
      "source": "/js/:path*",
      "destination": "https://marketing-celula.vercel.app/js/:path*"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}
